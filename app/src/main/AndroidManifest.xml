<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell_pc.health_first">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForgotPasswordActivity"></activity>
        <activity android:name=".ResetPasswordActivity"></activity>
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".ViewProfileActivity"></activity>
        <activity android:name=".UpdateProfileActivity"></activity>
    </application>

</manifest>